/* CIF PROCESS (250, 150), (150, 75) */
process Controller;
    /* CIF Keep Specific Geode Partition 'default' */
    /* CIF TEXT (1318, 142), (267, 140) */
    -- Text area for declarations and comments
    
    dcl check_param MyInteger;
    dcl result_param MyInteger;
    dcl error MyInteger;
    /* CIF ENDTEXT */
    /* CIF START (517, 227), (70, 35) */
    START;
        /* CIF task (489, 282), (124, 35) */
        task check_param := 1;
        /* CIF output (481, 337), (141, 35) */
        output check(check_param);
        /* CIF NEXTSTATE (517, 387), (70, 35) */
        NEXTSTATE Wait;
    /* CIF state (893, 204), (70, 35) */
    /* CIF Keep Specific Geode Partition 'default' */
    state Wait;
        /* CIF input (854, 259), (143, 35) */
        input result(result_param);
            /* CIF decision (861, 314), (129, 50) */
            decision result_param = 11;
                /* CIF ANSWER (848, 384), (70, 23) */
                (true):
                    /* CIF NEXTSTATE (848, 427), (70, 35) */
                    NEXTSTATE Wait;
                /* CIF ANSWER (938, 384), (70, 23) */
                (false):
                    /* CIF task (934, 427), (76, 35) */
                    task error := 1;
                    /* CIF NEXTSTATE (938, 477), (70, 35) */
                    NEXTSTATE Wait;
            enddecision;
    endstate;
endprocess Controller;