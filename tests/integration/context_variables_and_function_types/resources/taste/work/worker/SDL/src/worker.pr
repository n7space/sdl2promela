/* CIF PROCESS (250, 150), (150, 75) */
process type Worker;
    /* CIF Keep Specific Geode Partition 'default' */
    /* CIF TEXT (395, 245), (267, 140) */
    -- Text area for declarations and comments
    
    dcl check_param MyInteger;
    dcl result_param MyInteger;
    /* CIF ENDTEXT */
    /* CIF START (332, 26), (70, 35) */
    START;
        /* CIF NEXTSTATE (332, 76), (70, 35) */
        NEXTSTATE Wait;
    /* CIF state (1086, 169), (70, 35) */
    /* CIF Keep Specific Geode Partition 'default' */
    state Wait;
        /* CIF input (1048, 224), (141, 35) */
        input check(check_param);
            /* CIF task (962, 279), (313, 35) */
            task result_param := worker_ctxt.coeff + check_param;
            /* CIF output (1047, 334), (143, 35) */
            output result(result_param);
            /* CIF NEXTSTATE (1084, 389), (70, 35) */
            NEXTSTATE Wait;
    endstate;
endprocess type Worker;