system Complexsdl;
    /* CIF TEXT (552, 9), (291, 140) */
    use Datamodel comment 'dataview-uniq.asn';
    
    signal impulse(MyInteger);
    
    signal response(MyInteger);
    
    
    
    /* CIF ENDTEXT */
    channel c
        from env to Complexsdl with impulse;
        from Complexsdl to env with response;
    endchannel;
    block Complexsdl;
        signalroute r
            from env to Complexsdl with impulse;
            from Complexsdl to env with response;
        connect c and r;
        /* CIF PROCESS (250, 150), (150, 75) */
        process Complexsdl;
            /* CIF TEXT (1202, 0), (267, 140) */
            -- Text area for declarations and comments
            
            DCL tmp MyInteger;
            DCL param MyInteger;
            /* CIF ENDTEXT */
            /* CIF START (320, 21), (70, 35) */
            START;
                /* CIF NEXTSTATE (320, 71), (70, 35) */
                NEXTSTATE Wait;
            /* CIF state (450, 21), (70, 35) */
            state Wait;
                /* CIF input (433, 76), (101, 35) */
                input impulse(tmp);
                    /* CIF task (371, 131), (226, 35) */
                    task param := if tmp = 0 then 1 else 0 fi;
                    /* CIF output (423, 186), (122, 35) */
                    output response(param);
                    /* CIF NEXTSTATE (449, 236), (70, 35) */
                    NEXTSTATE Wait;
            endstate;
        endprocess Complexsdl;
    endblock;
endsystem;